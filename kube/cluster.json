apiVersion: eksctl.io/v1alpha5
kind: ClusterConfig
metadata:
  name: EKS-node-Cluster
  region: us-east-2

vpc:
  id: vpc-05849ee4f0b22a12b
  cidr: "192.168.0.0/16"
  subnets:
    public:
      us-east-2a:
        id: subnet-0daf0d68e9425031e
      us-east-2b:
        id: subnet-02f0671c456cfb55b
    private:
      us-east-2a:
        id: subnet-08fd9207fb0a9403b
      us-east-2b:
        id: subnet-0663e189fd0bab4d8

nodeGroups:
  - name: EKS-public-workers
    instanceType: t2.medium
    desiredCapacity: 2
  - name: EKS-private-workers
    instanceType: t2.medium
    desiredCapacity: 1
    privateNetworking: true